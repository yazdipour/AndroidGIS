            <circle fill="#FCCD32" stroke="#ff0000" stroke-width="0.2" radius="5" scale-radius="true" />
            <!--<caption style="bold" fill="#000000" k="name" priority="1" size="20"-->
            <!--stroke="#ffffff" stroke-width="2.0" />-->
            <!--<symbol src="assets:svg/lib.svg" />-->
